{
    "name": "jerodev/pokedex",
    "description": "Pokedex irc bot",
    "keywords": ["irc"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "jerodev/php-irc-client": "dev-master",
        "laravel/lumen-framework": "5.8.*",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
